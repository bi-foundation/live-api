        },
        "callbackType": {
          "description": "the type of callback: HTTP(S), HTTP with basic authentication, HTTP with OAUTH2 token",
          "x-go-name": "CallbackType"
        },
        "filters": {
          "description": "the emitted event can be filter to receive not all data from an event type",
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/Filter"
          },
          "x-go-name": "Filters"
        },
        "id": {
          "description": "the id of the subscription",
          "type": "string",
          "x-go-name": "Id",
          "readOnly": true
        },
        "model": {
          "$ref": "#/definitions/Model"
        }
      },
      "x-go-package": "github.com/FactomProject/live-api/EventRouter/models"
